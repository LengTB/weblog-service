<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/dtd/mybatis-3-mapper.dtd"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.tobycold.mapper.ArticleMapper">

    <insert id="save" useGeneratedKeys="true" keyProperty="id">
        insert into tb_context(title,tags,routeName,body,routeName,createUser,updateUser,updateTime,ip, seeCount, loveCount)
        values (${title},${tags},${routeName},${body},${routeName},${createUser},${updateUser},${updateTime},${ip}, ${seeCount}, ${loveCount})
    </insert>
    <delete id="delete">
        delete from tb_context
    </delete>
</mapper>